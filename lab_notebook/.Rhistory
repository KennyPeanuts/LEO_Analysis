# With Sea Level Rise
S <- c(0:99)
## Constants
b <- 3 + (0.1 * S)
q <- 10 + (2 * S)
k <- 0.1
m <- (b * (0.1 + (0.05 * S)))
e <- 10 + (0.1 * S)
## Model
I <- (b + (q * k))
O <- (m + e)
E <- I - O

# With Sea Level Rise
S <- c(0:99)
## Constants
b <- 51 + (0.1 * S) # value from Morris et al. 2002 figure 4
q <- 50 + (2 * S)
k <- 0.1
m <- (b * (0.1 + (0.05 * S))) # value from Langley et al. 2009 page 6183
e <- q * (0.5 + (0.1 * S)) # set to 50% of sediment delivery and increasing 10% with sea level rise
## Model
I <- (b + (q * k))
O <- (m + e)
E <- I - O
## Plots
plot(E ~ S, ylim = c(-100, 200))
points(O ~ S, pch = "-")
points(I ~ S, pch = "+")
abline(h = 0)
summary(lm(E ~ S))
## Plots
plot(E ~ S, ylim = c(-500, 500))
points(O ~ S, pch = "-")
points(I ~ S, pch = "+")
abline(h = 0)

q <- 50 + (2 * S) # sedimentation estimated to be equal to biomass accumulation
k <- 0.1 # biomass assumed to capture 10% of sediment
m <- (b * (0.1 + (0.05 * S))) # value from Langley et al. 2009 page 6183
e <- q * (0.5 + (0.1 * S)) # set to 50% of sediment delivery and increasing 10% with sea level rise
## Model
I <- (b + (q * k))
O <- (m + e)
E <- I - O
## Plots
plot(E ~ S, ylim = c(-50, 200), pch = 1)
points(O ~ S, pch = "-")
points(I ~ S, pch = "+")
abline(h = 0)
## Plots
plot(E ~ S, ylim = c(-100, 200), pch = 1)
points(O ~ S, pch = "-")
points(I ~ S, pch = "+")
abline(h = 0)
legend(0, 200, c("Marsh Elev.", "Accretion", "Loss"), pch = c("o", "+", "-"))
summary(lm(E ~ S))

plot(SlrkW ~ SlrkW_Avg, data = HSF.weather)
plot(SlrkW ~ TIMESTAMP, data = HSF.weather)
plot(WS_ms ~ WS_ms_Avg)
plot(WS_ms ~ WS_ms_Avg, data = HSF.weather)
